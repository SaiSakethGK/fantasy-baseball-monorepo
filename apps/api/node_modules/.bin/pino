#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/gksak/fantasy-baseball-monorepo/apps/api/node_modules/pino/node_modules:/mnt/c/Users/gksak/fantasy-baseball-monorepo/apps/api/node_modules:/mnt/c/Users/gksak/fantasy-baseball-monorepo/apps/node_modules:/mnt/c/Users/gksak/fantasy-baseball-monorepo/node_modules:/mnt/c/Users/gksak/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/gksak/fantasy-baseball-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/gksak/fantasy-baseball-monorepo/apps/api/node_modules/pino/node_modules:/mnt/c/Users/gksak/fantasy-baseball-monorepo/apps/api/node_modules:/mnt/c/Users/gksak/fantasy-baseball-monorepo/apps/node_modules:/mnt/c/Users/gksak/fantasy-baseball-monorepo/node_modules:/mnt/c/Users/gksak/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/gksak/fantasy-baseball-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pino/bin.js" "$@"
else
  exec node  "$basedir/../pino/bin.js" "$@"
fi
